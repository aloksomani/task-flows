<%= render "layouts/navbar" %>

<h3>Edit your Milestone</h3>

<%= form_for [@project, @milestone], url: project_milestone_path, method: "patch" do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <%= f.submit "Save Milestone" %>

<% end %>


<%= link_to "cancel", url_for(:back) %>
